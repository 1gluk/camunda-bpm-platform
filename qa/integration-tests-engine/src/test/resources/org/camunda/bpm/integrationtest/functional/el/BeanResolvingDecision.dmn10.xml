<?xml version="1.0" encoding="UTF-8"?>
<Definitions xmlns="http://www.omg.org/spec/DMN/20130901"
             xmlns:camunda="http://camunda.org/schema/1.0/dmn"
             id="definitions"
             name="camunda"
             namespace="http://camunda.org/dmn">
 
  <Decision id="decision" name="CheckOrder">
    <DecisionTable id="decisionTable" name="CheckOrder" isComplete="false" isConsistent="false">

      <clause id="clause1" name="CustomerStatus">
        <inputExpression id="inputExpression1" name="Input">
          <text>greeter.greet()</text>
        </inputExpression>
        <inputEntry id="inputEntry1">
          <text>"hello"</text>
        </inputEntry>
      </clause>

      <clause id="clause2" name="Check Result" camunda:output="result">
        <outputEntry id="outputEntry1">
          <text>"ok"</text>
        </outputEntry>
      </clause>

      <rule id="rule1">
        <condition>inputEntry1</condition>
        <conclusion>outputEntry1</conclusion>
      </rule>

    </DecisionTable>
  </Decision>
</Definitions>
